<resources xmlns:tools="http://schemas.android.com/tools">
    <style name="Theme.RentApp" parent="Theme.MaterialComponents.DayNight.DarkActionBar">
        <item name="colorPrimary">"#262626"</item>
        <item name="colorPrimaryVariant">"#262626"</item>
        <item name="colorOnPrimary">@color/black</item>

        <item name="colorSecondary">"#F1F0EA"</item>
        <item name="colorSecondaryVariant">"#F1F0EA"</item>
        <item name="colorOnSecondary">"#262626"</item>

        <item name="android:statusBarColor">?attr/colorPrimaryVariant</item>
    </style>

    <style name="Theme.App.Starting" parent="Theme.SplashScreen">
        <item name="windowSplashScreenBackground">#F1F0EA</item>
        <item name="windowSplashScreenAnimatedIcon">@mipmap/ic_launcher</item>
        <item name="windowSplashScreenAnimationDuration">300</item>
        <item name="postSplashScreenTheme">@style/Theme.RentApp</item>
    </style>
</resources>